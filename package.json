{
  "name": "yt-comments-downloader",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot index.ts",
    "start": "bun index.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fix": "bun run format && bun run lint:fix",
    "check": "concurrently 'bun run lint' 'bun run typecheck' 'bun run test'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "latest",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "happy-dom": "^20.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.54.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "neverthrow": "^8.2.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  }
}
